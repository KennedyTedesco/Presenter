{
  "name": "kennedytedesco/presenter",
  "description": "Presenters helps you clean up your views.",
  "keywords": ["view presenters", "presenter", "presenters", "view", "views"],
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Kennedy Tedesco",
      "email": "kennedyt.tw@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "illuminate/support": "5.3.*|5.4.*",
    "illuminate/container": "5.3.*|5.4.*"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "KennedyTedesco\\Presenter\\": "src/Presenter"
    },
    "files": ["src/helpers.php"]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    },
    "files": ["vendor/illuminate/support/helpers.php"]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  }
}
